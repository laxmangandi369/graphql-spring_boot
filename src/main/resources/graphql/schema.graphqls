type Query {
	allBooks:[Book]
	bookById(id: ID):Book
	authorById(id:ID): Author
}

type Mutation{
	createBook(Book book):Book

type Book {
	id:ID
	name: String
	description : String
	author : Author
	price:Float
	pages:Int
	
}

type Author{
	id:ID
	name:String
	dataTest : [DataTest]
}

type DataTest{
	id:ID
	name:String

}